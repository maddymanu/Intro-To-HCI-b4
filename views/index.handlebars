<!doctype html>

<html>
<head>
	<title>Momentz</title>	
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="css/bootstrap.min.css" rel="stylesheet">
	<link href="css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="css/introHCI.css" rel="stylesheet">
    {{> tracking_code_script}}
</head>

<body style="background-color:#088A85">
	<center>
		<img src="images/Momentz_logo.png" alt="Lorem Pixel image" class="img-responsive"  width="375px" >
	</center>
				
	<div class="container">
		<center>
		<!-- Added like buttons from sylvia.  -->
			<p style="color:white">Collaborative memory creating experience!</p>
		</center>
	</div>

	<div class="container">

	<form id="signupForm" role="form" method="get" action="/login">
	    
	    <div class="form-group">
	      <label for="width">Username:</label>
	      <input type="text" class="form-control" id="username" placeholder="Username" width="50">
	    </div>
    <div class="form-group">
      <label for="description">Password:</label>
      <input type="password" class="form-control" id="password" placeholder="Password">
    </div>
    <center>
    <button id="submitBtn" type="button" class="btn btn-default">Login</button>
    <button id="registerBtn" type="button" class="btn btn-default">Don't have an account? Sign Up!</button>
    </center>
  </form>

  <!-- Beginning Logging in  -->

	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="js/bootstrap.min.js"></script>
	<script src="js/introHCI.js"></script>
</div>
</body>
</html>
