<!doctype html>

<html>
<head>
	<title>Momentz</title>	
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="css/bootstrap.min.css" rel="stylesheet">
	<link href="css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="css/profileCss.css" rel="stylesheet">
    {{> tracking_code_script}}
</head>

<body>
<center>

		<!-- <div id="profileDetails">
			<a href="#"><img src="images/n6.jpg" /></a>
			<br>
		</div> -->
	<center>
		<div class="bg-header">
			<div>
				<p> Julian </p>
			</div>
		</div>
		<div class="profile-header">
			<img src="images/profile1.png" class = "img" float="left" height="80" width="80">
			<p><color="#000000"> "I love traveling and photography." </color><p>
		    <a href="http://www.linkedin.com/" target="_blank"><img border="0" src="images/linkedin.png" alt="LinkedIn Profile" width="40" height="40" /></a>
		    <a href="http://www.instagram.com" target="_blank"><img border="0" src="images/instagram_icon.png" alt="LinkedIn Profile" width="40" height="40" /></a>
		    <a href="https://www.facebook.com/" target="_blank"><img border="0" src="images/facebook.png" alt="LinkedIn Profile" width="40" height="40" /></a>
		    <a href="http://www.twitter.com" target="_blank"><img border="0" src="images/twitter.png" alt="LinkedIn Profile" width="40" height="40" /></a>
		</div>
	</center>

		<div class="container">

		{{#each stories}}
			<form id="viewStory" role="form" method="get" action="/story">
			<div id="{{id}}">

			<input type="image" src="{{img0}}" name="imageN" width="124" height="124">
			<!-- </a> -->
			<div class="details"></div>
			</div>
			<hr>

			<input type="hidden" name="title" value={{title}}>
			
			<a href="#" id="story"></a>

			</form>
		{{/each}}

	<center>
		<br>
		<a href="#" id="newstory"><button id="newStoryBtn" type="button">+ New Story</button></a>
	</center>

   <center>
    <br>
    <button id="logoutBtn" type="button" class="btn btn-default">Logout</button>
	</center>

	</div>

    <center>
    <ul id="menu">
		<li id="TopS" class="a_version_top_story"><div><img src="images/top.png" id="img"></div><div><a href="#">Top</a></div></li>
		<li id="NewsFeed" class="a_version_news_feed"><div><img src="images/newsfeed.png" id="img"></div><div><a href="#">Newsfeed</a></div></li>
		<li id="Publish"><div><img src="images/publish.png" id="img"></div><div><a href="#">Publish</a></div></li>
		<li id="Profile"><div><img src="images/profile2.png" id="img"></div><div><a href="#" style="color:#40FF00">Profile</a></div></li>

	</ul>
	</center>
	</div>	
	

	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="js/bootstrap.min.js"></script>
	<script src="js/introHCI.js"></script>
	</center>
</body>
</html>
